// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model User {
  id String
  first_name String
  last_name String
  phone String @unique
  parent_phone String
  email String  @unique @id
  pass String
}
model Course {
  id String @id 
  name String  
  des String
  price Int
  thumbnail String
  vidlink String
}